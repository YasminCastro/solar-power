{
  "200": {
    "description": "Lista de achievements obtida com sucesso.",
    "content": {
      "application/json": {
        "schema": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "_id": {
                "type": "string",
                "description": "ID único do achievement."
              },
              "name": {
                "type": "string",
                "description": "Nome do achievement."
              },
              "description": {
                "type": "string",
                "description": "Descrição detalhada do achievement."
              },
              "points": {
                "type": "integer",
                "description": "Pontos atribuídos ao conquistar o achievement."
              },
              "createdAt": {
                "type": "string",
                "format": "date-time",
                "description": "Data e hora da criação do achievement."
              }
            }
          }
        }
      }
    }
  },
  "401": {
    "description": "Não autorizado. Token de autenticação está ausente, inválido ou expirado."
  }
}
